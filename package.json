{
  "name": "ts-publisher",
  "version": "0.0.1",
  "repository": "git@github.com:suguru03/ts-publisher.git",
  "author": "Suguru Motegi <suguru.motegi@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc"
  },
  "bin": {
    "tsp": "./bin/tsp.js"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "inquirer": "^6.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "^0.0.44",
    "@types/node": "^11.9.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
